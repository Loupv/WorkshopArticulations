
class VProSample {
  
  float trig;
  float ch1;
  float ch2;
  float ch3;
  float ch4;
  float ch5;
  float ch6;
  float ch7;
  float ch8;
  int time;
  int id;
  
  VProSample(float trig, float ch1, float ch2, float ch3, float ch4, float ch5, float ch6,
      float ch7, float ch8, int id, int time)
  {
    this.trig = trig;
    this.ch1 = ch1;
    this.ch2 = ch2;
    this.ch3 = ch3;
    this.ch4 = ch4;
    this.ch5 = ch5;
    this.ch6 = ch6;
    this.ch7 = ch7;
    this.ch8 = ch8;
    this.id = id;
    this.time = time;
  }
  
}